package com.telusko.SpringAppWithAnnotationBasedApproach;

public class Apple {

	MobileProcessor cpu;
	
	
	public MobileProcessor getCpu() {
		return cpu;
	}

	public void setCpu(MobileProcessor cpu) {
		this.cpu = cpu;
	}

	public void config() {
		System.out.println("Octa Core, 4GB RAM");
		cpu.process();
	}
}
