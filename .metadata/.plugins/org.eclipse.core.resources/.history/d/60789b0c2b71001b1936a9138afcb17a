
public class AllFactorsChallenge {

	public static void main(String[] args) {	
		
		printFactors(100);
		
		
		
	}
	public static void printFactors(int number) {
	
		for(int n=1; n<= number; n++) {
			
			if(n % number == 0) {
				
				System.out.print("The factors for number " + number + " are : " + n);
				
			}
			
		}
	}

}
